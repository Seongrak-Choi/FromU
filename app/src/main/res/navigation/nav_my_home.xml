<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_my_home"
    app:startDestination="@id/myHomeFragment">

    <fragment
        android:id="@+id/myHomeFragment"
        android:name="com.fromu.fromu.ui.main.myhome.MyHomeFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_my_home">
        <action
            android:id="@+id/action_myHomeFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
        <action
            android:id="@+id/action_myHomeFragment_to_alarmMsgSettingFragment"
            app:destination="@id/alarmMsgSettingFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.fromu.fromu.ui.main.myhome.setting.SettingFragment"
        android:label="SettingFragment"
        tools:layout="@layout/fragment_setting" />
    <fragment
        android:id="@+id/alarmMsgSettingFragment"
        android:name="com.fromu.fromu.ui.main.myhome.AlarmMsgSettingFragment"
        android:label="AlarmMsgSettingFragment"
        tools:layout="@layout/fragment_alarm_msg_setting" />
</navigation>