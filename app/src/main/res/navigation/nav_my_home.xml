<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_my_home"
    app:startDestination="@id/myHomeFragment">

    <fragment
        android:id="@+id/myHomeFragment"
        android:name="com.fromu.fromu.ui.main.myhome.MyHomeFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_my_home">
        <action
            android:id="@+id/action_myHomeFragment_to_settingFragment"
            app:destination="@id/settingFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_enter_anim"
            app:popExitAnim="@anim/nav_default_exit_anim"
            />
        <action
            android:id="@+id/action_myHomeFragment_to_alarmMsgSettingFragment"
            app:destination="@id/alarmMsgSettingFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_enter_anim"
            app:popExitAnim="@anim/nav_default_exit_anim"
            />
        <action
            android:id="@+id/action_myHomeFragment_to_patchMailBoxNameFragment"
            app:destination="@id/patchMailBoxNameFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_enter_anim"
            app:popExitAnim="@anim/nav_default_exit_anim"/>
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.fromu.fromu.ui.main.myhome.setting.SettingFragment"
        android:label="SettingFragment"
        tools:layout="@layout/fragment_setting">
        <action
            android:id="@+id/action_settingFragment_to_withdrawalCheckFragment"
            app:destination="@id/withdrawalCheckFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_enter_anim"
            app:popExitAnim="@anim/nav_default_exit_anim" />
        <action
            android:id="@+id/action_settingFragment_to_breakMatchingCheckFragment"
            app:destination="@id/breakMatchingCheckFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_enter_anim"
            app:popExitAnim="@anim/nav_default_exit_anim" />
        <action
            android:id="@+id/action_settingFragment_to_appInfoFragment"
            app:destination="@id/appInfoFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_enter_anim"
            app:popExitAnim="@anim/nav_default_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/alarmMsgSettingFragment"
        android:name="com.fromu.fromu.ui.main.myhome.AlarmMsgSettingFragment"
        android:label="AlarmMsgSettingFragment"
        tools:layout="@layout/fragment_alarm_msg_setting" />

    <fragment
        android:id="@+id/withdrawalCheckFragment"
        android:name="com.fromu.fromu.ui.main.myhome.setting.WithdrawalCheckFragment"
        android:label="WithdrawalCheckFragment"
        tools:layout="@layout/fragment_withdrawal_check" />
    <fragment
        android:id="@+id/breakMatchingCheckFragment"
        android:name="com.fromu.fromu.ui.main.myhome.setting.BreakMatchingCheckFragment"
        android:label="BreakMatchingCheckFragment"
        tools:layout="@layout/fragment_break_matching_check" />
    <fragment
        android:id="@+id/appInfoFragment"
        android:name="com.fromu.fromu.ui.main.myhome.setting.AppInfoFragment"
        android:label="AppInfoFragment"
        tools:layout="@layout/fragment_app_info" />
    <fragment
        android:id="@+id/patchMailBoxNameFragment"
        tools:layout="@layout/fragment_patch_mail_box_name"
        android:name="com.fromu.fromu.ui.main.myhome.PatchMailBoxNameFragment"
        android:label="PatchMailBoxNameFragment" />
</navigation>