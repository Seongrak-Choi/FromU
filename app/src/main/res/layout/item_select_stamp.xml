<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data></data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        android:layout_marginVertical="20dp">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_select_stamp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:src="@drawable/ic_letter_stamp_1" />

        <View
            android:id="@+id/v_select"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/bg_rectangle_empty_stroke_a735ff_radius_8dp_width_2dp"
            app:layout_constraintBottom_toBottomOf="@+id/iv_select_stamp"
            app:layout_constraintEnd_toEndOf="@+id/iv_select_stamp"
            app:layout_constraintStart_toStartOf="@+id/iv_select_stamp"
            app:layout_constraintTop_toTopOf="@+id/iv_select_stamp" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>